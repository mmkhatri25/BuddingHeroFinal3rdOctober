      \u043F\u043E \u0438\u0433\u0440\u0435!"
    - "\u041F\u041E\u0411\u0420\u042F\u041A\u0423\u0428\u041A\u0418"
    - "\u041F\u043E\u0431\u0440\u044F\u043A\u0443\u0448\u043A\u0438 \u2013 \u044D\u0442\u043E
      \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u043E\u0435 \u043E\u0431\u043E\u0440\u0443\u0434\u043E\u0432\u0430\u043D\u0438\u0435,
