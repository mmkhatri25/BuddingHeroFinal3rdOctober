      \u043F\u043E\u0431\u0440\u044F\u043A\u0443\u0448\u043A\u0438, \u043E\u0434\u043D\u0430\u043A\u043E
      \u043A\u0430\u0436\u0434\u0430\u044F \u043F\u043E\u0431\u0440\u044F\u043A\u0443\u0448\u043A\u0430
      \u043C\u043E\u0436\u0435\u0442 \u0438\u043C\u0435\u0442\u044C \u0434\u043E 3
      \u044D\u0444\u0444\u0435\u043A\u0442\u043E\u0432!\n</br>\u041A\u0443\u043F\u0438\u0442\u0435
      \u043A\u043E\u0440\u043E\u0431\u043A\u0443 \u0438 \u0432\u044B \u043F\u043E\u043B\u0443\u0447\u0438\u0442\u044C
