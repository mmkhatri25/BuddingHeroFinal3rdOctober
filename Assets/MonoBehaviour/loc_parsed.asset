      \u0447\u0442\u043E \u044D\u0442\u043E \u043F\u0440\u0438\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u0435
      \u0441\u0434\u0435\u043B\u0430\u0435\u0442 \u0438\u0437 \u043D\u0435\u0433\u043E
      \u043F\u0435\u0440\u0432\u043E\u043A\u043B\u0430\u0441\u0441\u043D\u043E\u0433\u043E
      \u0432\u043E\u043B\u0448\u0435\u0431\u043D\u0438\u043A\u0430."
    - "\u042D\u043A\u0441\u043F\u0435\u0440\u0438\u043C\u0435\u043D\u0442\u044B \u0411\u0430-\u0434\u0430-\u0431\u0443\u043C\u0441\u0430
