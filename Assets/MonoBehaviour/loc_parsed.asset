      \u0432 \u043C\u0435\u043D\u044E \u0414\u043E\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0435
      \u043E\u043F\u0446\u0438\u0438."
    - "\u0418\u043D\u043E\u0433\u0434\u0430 \u043E\u0431\u044B\u0447\u043D\u044B\u0439
      \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442 \u043E\u0431\u043B\u0430\u0434\u0430\u0435\u0442
      \u0431\u043E\u043B\u0435\u0435 \u043B\u0443\u0447\u0448\u0438\u043C\u0438 \u0445\u0430\u0440\u0430\u043A\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043A\u0430\u043C\u0438,
      \u0447\u0435\u043C \u044D\u043F\u0438\u0447\u0435\u0441\u043A\u0438\u0439."
    - "\u0411\u0430\u043B\u0430\u0431\u043E\u043B\u0443\u0441 \u043D\u0435 \u043C\u043E\u0436\u0435\u0442
      \u0447\u0438\u0442\u0430\u0442\u044C, \u043F\u043E\u0442\u043E\u043C\u0443 \u0447\u0442\u043E
      \u043E\u043D \u043F\u043E\u0442\u0435\u0440\u044F\u043B \u043E\u0447\u043A\u0438.
      \u0418 \u043E\u0442\u043A\u0430\u0437\u044B\u0432\u0430\u0435\u0442\u0441\u044F
      \u0438\u0445 \u0438\u0441\u043A\u0430\u0442\u044C."
    - "\u041B\u0438\u044F \u043A\u0441\u0442\u0430\u0442\u0438 \u043E\u0447\u0435\u043D\u044C
      \u043D\u0435\u043F\u043B\u043E\u0445\u043E \u0432\u0438\u0434\u0438\u0442, \u0433\u043E\u043B\u043E\u0432\u0430
      \u043F\u0440\u0430\u0432\u0434\u0430 \u0443 \u043D\u0435\u0435 \u0441\u043E\u0432\u0441\u0435\u043C
      \u043D\u0435 \u0432\u0430\u0440\u0438\u0442."
    - "\u0422\u044B \u043C\u043E\u0436\u0435\u0448\u044C \u0432\u044B\u0431\u0440\u0430\u0442\u044C
      \u043D\u0430\u0440\u044F\u0434\u044B \u0434\u043B\u044F \u0441\u0432\u043E\u0438\u0445
      (\u043F\u043E\u0447\u0442\u0438) \u0433\u0435\u0440\u043E\u0435\u0432!"
    - "\u041D\u0435\u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u0434\u043E\u0440\u043E\u0433\u0438\u0435
      \u043D\u0430\u0440\u044F\u0434\u044B \u0438\u043C\u0435\u044E\u0442 \u0434\u043E\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0435
      \u0432\u0438\u0437\u0443\u0430\u043B\u044C\u043D\u044B\u0435 \u044D\u0444\u0444\u0435\u043A\u0442\u044B!"
    - "\u041A\u0440\u0430\u0441\u0430\u0432\u0447\u0438\u043A \u0414\u0436\u0438\u043C
      \u043E\u0431\u043E\u0436\u0430\u0435\u0442 \u043C\u0443\u0437\u044B\u043A\u0443.
      \u041D\u043E \u0435\u0449\u0435 \u0431\u043E\u043B\u044C\u0448\u0435 \u043E\u043D
      \u043E\u0431\u043E\u0436\u0430\u0435\u0442 \u0441\u0432\u043E\u0435 \u043B\u0438\u0446\u043E."
    - "\u0416\u0434\u0435\u0448\u044C \u043E\u0447\u0435\u0440\u0435\u0434\u043D\u043E\u0435
      \u043C\u0438\u043D\u0438-\u0437\u0430\u0434\u0430\u043D\u0438\u0435? \u041F\u0440\u043E\u0432\u0435\u0440\u044C
      \u043E\u0431\u0440\u0430\u0442\u043D\u044B\u0439 \u043E\u0442\u0441\u0447\u0435\u0442
      \u0432 \u043C\u0435\u043D\u044E \u0434\u043E\u0441\u0442\u0438\u0436\u0435\u043D\u0438\u0439."
    - "\u0412\u0435\u043A\u0441 \u043B\u044E\u0442\u043E \u0434\u0443\u0431\u0430\u0441\u0438\u0442
      \u0432\u0440\u0430\u0433\u043E\u0432 \u043C\u043E\u043B\u043E\u0442\u043A\u043E\u043C.
      \u041D\u043E \u0434\u0430\u0436\u0435 \u0435\u0439 \u043D\u0443\u0436\u0435\u043D
      \u043E\u0442\u0434\u044B\u0445."
    - "\u0422\u044B \u043C\u043E\u0436\u0435\u0448\u044C \u043F\u043E\u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C
      \u0431\u043E\u043D\u0443\u0441\u044B \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u0430,
      \u043D\u0430\u0436\u0430\u0432 \u043D\u0430 \u0437\u043D\u0430\u0447\u043E\u043A
      \xABi\xBB \u043D\u0430 \u0432\u043A\u043B\u0430\u0434\u043A\u0435."
    - "\u041C\u0438\u0444\u0438\u0447\u0435\u0441\u043A\u0438\u0435 \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u044B
      \u043D\u0435\u043B\u044C\u0437\u044F \u0438\u0437\u043C\u0435\u043D\u0438\u0442\u044C,
      \u043D\u043E \u0438\u0445 \u043C\u043E\u0436\u043D\u043E \u043F\u0440\u043E\u043A\u0430\u0447\u0430\u0442\u044C."
    - "(\u041F\u043E\u0447\u0442\u0438) \u0432\u0441\u0435, \u0447\u0442\u043E \u0445\u043E\u0447\u0435\u0442
      \u0422\u044D\u043C \u2013 \u044D\u0442\u043E \u0447\u0442\u043E\u0431\u044B
      \u0421\u044D\u043C \u0435\u0435 \u043F\u0440\u043E\u0441\u0442\u0438\u043B."
    - "\u0412 \u0438\u0433\u0440\u0435 \u0432\u0441\u0435\u0433\u043E 40 \u044D\u0444\u0444\u0435\u043A\u0442\u043E\u0432
      \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u043E\u0432 \u0441 \u043C\u0430\u043A\u0441\u0438\u043C\u0430\u043B\u044C\u043D\u044B\u043C
      \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u0435\u043C. \u0421\u043E\u0431\u0435\u0440\u0435\u0448\u044C
      \u0438\u0445 \u0432\u0441\u0435?"
    - "\u041E\u0441\u0442\u043E\u0440\u043E\u0436\u043D\u043E, \u0435\u0441\u043B\u0438
      \u0442\u044B \u043C\u0435\u043D\u044F\u0435\u0448\u044C \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442,
      \u043D\u0435 \u0444\u0430\u043A\u0442, \u0447\u0442\u043E \u043D\u043E\u0432\u044B\u0439
      \u0431\u0443\u0434\u0435\u0442 \u043B\u0443\u0447\u0448\u0435!"
    - "\u0410\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u044B \u043D\u0435 \u0431\u0443\u0434\u0443\u0442
      \u0438\u043C\u0435\u0442\u044C \u044D\u0444\u0444\u0435\u043A\u0442\u043E\u0432
      \u0432\u043E \u0412\u0440\u0430\u0442\u0430\u0445 \u0413\u043E\u0433\u0430."
    - "\u0412\u0430\u0448\u0438 \u0430\u043C\u0443\u043B\u0435\u0442\u044B \u043C\u043E\u0433\u0443\u0442
      \u043F\u043E\u044F\u0432\u0438\u0442\u044C\u0441\u044F \u0432\u043E \u0412\u0440\u0430\u0442\u0430\u0445
      \u0413\u043E\u0433\u0430."
    - "\u0420\u0435\u0434\u0440\u043E\u0445 \u0440\u0430\u043D\u044C\u0448\u0435 \u0441\u043E\u0431\u0438\u0440\u0430\u043B
      \u0432\u0441\u0435, \u0447\u0442\u043E \u0431\u043B\u0435\u0441\u0442\u0438\u0442.
      \u041E\u043D \u0438\u0437\u043C\u0435\u043D\u0438\u043B\u0441\u044F. \u0421\u0435\u0439\u0447\u0430\u0441
      \u044D\u0442\u043E \u0442\u043E\u043B\u044C\u043A\u043E \u0437\u043E\u043B\u043E\u0442\u043E."
    - "\u0420\u0443\u043D\u044B \u0441 \u0430\u0432\u0442. \u044D\u0444\u0444\u0435\u043A\u0442\u0430\u043C\u0438
      \u043D\u0435 \u0432\u043B\u0438\u044F\u044E\u0442 \u043D\u0430 \u041F\u0440\u0435\u043E\u0431\u0440\u0430\u0436\u0430\u0442\u0435\u043B\u044C
      \u0438\u043B\u0438 \u041E\u0431\u0449\u0438\u0435 \u043F\u0440\u0438\u0441\u0442\u0440\u0430\u0441\u0442\u0438\u044F."
    - "\u0414\u0430\u0440 \u043B\u0435\u043D\u0438\u0432\u0446\u0443 \u0431\u0443\u0434\u0435\u0442
      \u043F\u043E\u043A\u0443\u043F\u0430\u0442\u044C \u0443\u043B\u0443\u0447\u0448\u0435\u043D\u0438\u044F
      \u044D\u0442\u0430\u043F\u043E\u0432, \u043A\u0430\u043A \u0442\u043E\u043B\u044C\u043A\u043E
      \u043E\u043D \u0434\u043E\u0441\u0442\u0438\u0433\u043D\u0435\u0442 91 \u0443\u0440\u043E\u0432\u043D\u044F."
    - "\u0423\u043D\u043E \u0440\u0430\u043D\u044C\u0448\u0435 \u0431\u044B\u043B\u044C
      \u043F\u043B\u043E\u0445\u0438\u0448\u043E\u043C. \u041E\u043D \u0438\u0441\u043F\u0440\u0430\u0432\u0438\u043B\u0441\u044F.
      \u041F\u043E\u0447\u0442\u0438 \u0447\u0442\u043E."
    - "\u041F\u0440\u0435\u0434\u043C\u0435\u0442\u044B \u043A\u0443\u043F\u0446\u0430
      \u0441\u0442\u043E\u044F\u0442 \u0436\u0435\u0442\u043E\u043D\u043E\u0432 \u0438
      \u043C\u043E\u0433\u0443\u0442 \u0431\u044B\u0442\u044C \u043F\u043E\u043B\u0435\u0437\u043D\u044B
      \u0432 \u0431\u043E\u044E!"
    - "\u041D\u0430\u0436\u043C\u0438 \u043D\u0430 \u043A\u043D\u043E\u043F\u043A\u0443
      \u0418\u041D\u0424\u041E\u0420\u041C\u0410\u0426\u0418\u042F \u041E\u0411 \u0418\u0413\u0420\u0415,
      \u0447\u0442\u043E\u0431\u044B \u043F\u043E\u043B\u0443\u0447\u0438\u0442\u044C
      \u043C\u043E\u0440\u0435 \u043F\u043E\u043B\u0435\u0437\u043D\u044B\u0445 \u043F\u043E\u0434\u0441\u043A\u0430\u0437\u043E\u043A."
    - "\u0423 \u0411\u0430\u0431\u0443\u0441\u0438 \u0434\u043B\u044F \u0413\u043E\u0433\u043E\u043B\u044B
      \u043F\u0440\u0438\u0433\u043E\u0442\u043E\u0432\u043B\u0435\u043D \u0431\u0443\u043B\u044C\u043E\u043D
      \u0438 \u043E\u0431\u043D\u0438\u043C\u0430\u0448\u043A\u0438. \u0412\u0441\u0435
      \u0432\u0435\u0440\u043D\u043E, \u043E\u043D\u0430 \u043E\u0431\u0440\u0435\u0447\u0435\u043D\u0430."
    - "\u0411\u043E\u043D\u0443\u0441\u044B \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u043E\u0432
      \u043F\u043E\u043C\u043E\u0433\u0430\u044E\u0442 \u0412\u0421\u0415\u041C \u0433\u0435\u0440\u043E\u044F\u043C,
      \u0438 \u0434\u0430\u0436\u0435 \u0442\u0435\u043C, \u043A\u043E\u0442\u043E\u0440\u044B\u0445
      \u0442\u044B \u0435\u0449\u0435 \u043D\u0435 \u043E\u0442\u043A\u0440\u044B\u043B!"
