    - "\u041F\u0420\u0415\u0414\u041C\u0415\u0422\u042B"
    - "\u0423 \u043A\u0430\u0436\u0434\u043E\u0433\u043E \u0433\u0435\u0440\u043E\u044F
      \u0435\u0441\u0442\u044C 3 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u0430:
      \u0421\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u0435\u0442 \u043D\u0435\u0441\u043A\u043E\u043B\u044C\u043A\u043E
      \u0443\u0440\u043E\u0432\u043D\u0435\u0439 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u043E\u0432:
      {0}, {1}, {2}, {3}, {4}, {5}.\n</br>\u0414\u043B\u044F \u0442\u043E\u0433\u043E
      \u0447\u0442\u043E\u0431\u044B \u0440\u0430\u0437\u0432\u0438\u0432\u0430\u0442\u044C
      \u0433\u0435\u0440\u043E\u0435\u0432, \u0432\u0430\u043C \u043D\u0443\u0436\u043D\u044B
      \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u044B. \u0412\u0441\u0435 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u044B,
      \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u0443 \u0432\u0430\u0441 \u0435\u0441\u0442\u044C,
      \u0443\u043B\u0443\u0447\u0448\u0430\u044E\u0442 \u0432\u0430\u0448\u0438 \u0445\u0430\u0440\u0430\u043A\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043A\u0438,
      \u0434\u0430\u0436\u0435 \u0435\u0441\u043B\u0438 \u043E\u043D\u0438 \u0434\u043B\u044F
      \u0433\u0435\u0440\u043E\u044F, \u043A\u043E\u0442\u043E\u0440\u043E\u0433\u043E
      \u0432\u044B \u043D\u0435 \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u0443\u0435\u0442\u0435,
      \u0438\u043B\u0438 \u043A\u043E\u0442\u043E\u0440\u043E\u0433\u043E \u0443 \u0432\u0430\u0441
      \u0435\u0449\u0435 \u043D\u0435\u0442."
    - "\u0421\u0423\u041D\u0414\u0423\u041A\u0418"
    - "\u0412 \u043C\u0430\u0433\u0430\u0437\u0438\u043D\u0435 \u0432\u0430\u0441
      \u0436\u0434\u0435\u0442 \u043C\u043D\u043E\u0433\u043E \u0445\u043E\u0440\u043E\u0448\u0438\u0445
      \u043F\u0440\u0435\u0434\u043B\u043E\u0436\u0435\u043D\u0438\u0439, \u043A\u043E\u0442\u043E\u0440\u044B\u0435
