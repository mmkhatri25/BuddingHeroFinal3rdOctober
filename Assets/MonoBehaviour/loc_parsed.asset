      \u0431\u044B\u0442\u044C \u043D\u0435 \u0431\u043E\u043B\u044C\u0448\u0435 \u043E\u0434\u043D\u043E\u0439
      \u043F\u043E\u0431\u0440\u044F\u043A\u0443\u0448\u043A\u0438."
    - "\u0418\u0421\u041F\u042B\u0422\u0410\u041D\u0418\u042F \u041D\u0410 \u0412\u0420\u0415\u041C\u042F"
    - "\u0418\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u044F, \u043F\u0440\u0438\u0434\u0443\u043C\u0430\u043D\u043D\u044B\u0435
      \u0417\u0435\u043B\u0435\u043D\u044B\u043C \u0427\u0435\u043B\u043E\u0432\u0435\u043A\u043E\u043C,
      \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u0433\u043E\u0442\u043E\u0432\u044F\u0442
      \u0432\u0430\u0441 \u043A \u0447\u0435\u043C\u0443-\u0442\u043E \u0431\u043E\u043B\u0435\u0435
      \u0433\u0440\u0430\u043D\u0434\u0438\u043E\u0437\u043D\u043E\u043C\u0443.\n</br>\u0418\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u044F
      \u043E\u0433\u0440\u0430\u043D\u0438\u0447\u0438\u0432\u0430\u044E\u0442 \u0432\u0430\u0441
