      \u0432\u044B \u043F\u043E\u043B\u0443\u0447\u0438\u043B\u0438 \u043F\u0440\u0435\u0434\u043C\u0435\u0442,
      \u043A\u043E\u0442\u043E\u0440\u044B\u0439 \u0443 \u0432\u0430\u0441 \u0443\u0436\u0435
      \u0435\u0441\u0442\u044C, \u0431\u043E\u043B\u044C\u0448\u0435\u0435 \u043B\u0443\u0447\u0448\u0430\u044F
      \u0432\u0435\u0440\u0441\u0438\u044F \u0441\u043E\u0445\u0440\u0430\u043D\u0438\u0442\u0441\u044F
      \u0432 \u0438\u043D\u0432\u0435\u043D\u0442\u0430\u0440\u0435, \u0430 \u0445\u0443\u0434\u0448\u0430\u044F
      \u0432\u0435\u0440\u0441\u0438\u044F \u043F\u043E\u0439\u0434\u0435\u0442 \u043D\u0430
