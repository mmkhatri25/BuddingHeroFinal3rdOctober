      \u043E\u0434\u043D\u0430\u043A\u043E \u0432 \u043D\u0438\u0445 \u0432\u044B
      \u0441\u043C\u043E\u0436\u0435\u0442\u0435 \u043F\u043E\u043B\u0443\u0447\u0438\u0442\u044C
      \u043D\u043E\u0432\u044B\u0435 \u0442\u043E\u0432\u0430\u0440\u044B. \u0412\u044B
      \u043C\u043E\u0436\u0435\u0442\u0435 \u043F\u0440\u0435\u0440\u0432\u0430\u0442\u044C
      \u0438\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u0435 \u0432 \u043B\u044E\u0431\u043E\u0439
      \u043C\u043E\u043C\u0435\u043D\u0442, \u0435\u0441\u043B\u0438 \u043E\u043D\u043E
      \u043E\u043A\u0430\u0437\u0430\u043B\u043E\u0441\u044C \u0441\u043B\u0438\u0448\u043A\u043E\u043C
